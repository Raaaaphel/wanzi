					if (result.code == 1000) {
						_this.html(num + 's');
						_this.css('background-color', '#CCCCCC');
						_this.attr('disabled', true);
						clearInterval(timer);
						timer = setInterval(function() {
							num--;
							_this.css('background-color', '#CCCCCC');
							_this.html(num + 's');
							if (num == 0) {
								isCheck = false;
								_this.css('background-color', '#77C0CD');
								_this.html('重新获取');
								_this.attr('disabled', false);
