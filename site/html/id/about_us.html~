					} else {
						$().popup(result.error);
					}
				}, 'json');
			});
			//注册
			$('#register').submit(function() {
				$().loading();
				$.post('/user/register', $(this).serialize(), function(result) {
					$().unload();
					if (result.code == 1000) {
						location.reload();
					} else {
						$().popup(result.error);
					}
				}, 'json');
				return false;
			});
			//登录
			$('#login').submit(function() {
				$().loading();
				$.post('/user/login', $(this).serialize(), function(result) {
					$().unload();
