			});
			//第三方注册绑定手机号
			$('#phone').submit(function(){
				var prefix = $(this).find('.select-code').val(), phone =$(this).find('.phone-code').val(),code=$(this).find('.yan-code').val();
				if(!phone||!code){
					$().showMessage({'type': 'close','message': '手机号和验证码不能为空'});
					return false;
				}
				$().loading();
				$.post('/user/openRegister', {'phone':prefix+phone,'code':code}, function(result) {
					$().unload();
					if (result.code == 1000) {
						window.location.href='/';
					} else {
						$().popup(result.error);
					}
				}, 'json');
				return false;
			});
			//根据手机号修改密码
			$('#epassword').submit(function(){
				$().loading();
				$.post('/user/resetPassword',$(this).serialize(),function(result){
					$().unload();
					if((result.code==1000)||(result.code==1010)){
						$().popup('密码变更成功');
						$('.login-modal').modal('show');
						$('.forget-password').modal('hide');
						resterLogin(true);
					}else{
						$().popup(result.error);
					}
				},'json');
				return false;
			});
			
		})*/
		//登出
		$('.out').live('click',function(){
			$().showMessage('warning','确定要退出登录？',{
				'ensure':function(){
					$.post('/user/logout',{},function(result){
						if(result.code==1000){
							location.reload();
						}else{
							$().popup(result.error);
						}
					},'json');
				},
				'quite':function(){}
			});
		});
		if(!isMobile()){
			//电脑端顶部帮助中心
			$('.help-center').hover(function() {
				clearTimeout(timer4);
				$(this).children('ul').show();
			}, function() {
				timer4 = setTimeout(function() {
					$('.help-center').children('ul').hide();
				}, 200)
			});
					//电脑端顶部下载APP
			$('.load-img').hover(function(){
				clearInterval(timer1);
				$('.load-section').show();
			},function(){
				timer1 = setTimeout(function(){
					$('.load-section').hide();
				}, 200);
			});
		}
		//photoChange();
		//手机端底部箭头点击
		$('.arrow-btn').on('click', function() {
			$('.arrow-btn').removeClass('current');
			$('.hidden-section').slideUp();
			if ($(this).parent().siblings('.hidden-section').css('display') == 'none') {
				$(this).addClass('current');
				$(this).parent().siblings('.hidden-section').slideDown();
			}
			else {
				$(this).removeClass('current');
				$(this).parent().siblings('.hidden-section').slideUp();
			}
		});
		//注册登录电脑端
